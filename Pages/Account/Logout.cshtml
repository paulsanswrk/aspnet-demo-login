@page
@model IdentityPortal.Pages.Account.LogoutModel
@{
    ViewData["Title"] = "Logged Out";
}

<div class="auth-container">
    <div class="auth-card">
        <div class="auth-header">
            <div class="auth-icon"><i data-lucide="log-out"></i></div>
            <h1>Signed Out</h1>
            <p>You have been successfully signed out.</p>
        </div>
        <div class="auth-footer" style="margin-top: 1.5rem;">
            <a asp-page="/Account/Login" class="btn btn-primary btn-block">Sign In Again</a>
        </div>
    </div>
</div>
